#
# copy to gce.env and configure, then
# $ source gce.env
# 
# the GCE email address 
export GCE_EMAIL=

# the name of of the gce project
export GCE_PROJECT=cochroachdb-cluster

# the path to the gce credentials file  
export GCE_CREDENTIALS_FILE_PATH=

# the gce zone, i.e. europe-west
export GCE_ZONE=europe-west